import maths
def perfectsquares(n):
    dp = [float('inf')] * (n + 1)
    dp[0] = 0
    for i in range(1,n+1):
        for j in range(1,int(maths.sqrt(i))+1):
            dp[i]=min(dp[i],dp[i-j*j]+1)
    return dp[n]
print(perfectsquaresa(12))
print(perfectsquaresa(13))
print(perfectsquaresa(1))
print(perfectsquaresa(4))
print(perfectsquaresa(3))
